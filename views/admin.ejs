  <script src="https://unpkg.com/bootstrap-table@1.15.5/dist/extensions/export/bootstrap-table-export.min.js"></script>
  <script src="https://unpkg.com/bootstrap-table@1.15.5/dist/bootstrap-table-locale-all.min.js"></script>
  <link rel="stylesheet" href="/public/css/style.css" type="text/css"/>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<body>
  <div class="container">
    <nav class="navbar navbar-default">
      <div class="container-fluid">
        <div id="navbar" class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
            <li> <img src="/public/img/logo2.png" with="94" height="45" alt="logo" srcset=""></li>
            <li class="active"><a href="/home/logout">Powtót</a></li>
            <li ><a href="/home/new_list">Lista zamówień</a></li>
            <li ><a href="/home/raport">Raport</a></li>
          </ul>
          <ul class="nav navbar-nav navbar-right">
            <li><a href="/home/logout">Wylogowanie</a></li>
          </ul>
        </div>
      </div>
    </nav>
    <div class="jumbotron">
        <p>Witaj w panelu administracyjnym</p>
        <div class="level-1">
          <p>zarządzanie użytkownikami:</p>
          <div class="addUser">
            <p>dodaj lub uaktualnij użytkownika</p>
              id <input disabled type="text" id="myid" ><br><br> 
              SESA <input type="text" id="sesa_no"><br><br>
              Imię <input type="text" id="first_name"><br><br>
              Nazwisko <input type="text" id="last_name"><br><br>
              Telefon <input type="text" id="mob_no"><br><br>
              Sktót <input type="text" id="user_name"><br><br>
              Hasło <input type="password" id="password"><br><br>
              <input  id="btn_add" type="submit"  onclick="addUser1()" value="Dodaj">   <br>
              <input hidden id ="btn_update" type="submit"  onclick="updateDB()" value="Uaktualnij">           

          </div>
          <br>
          <div class="tableUser">
            <table
            id="table"
            class="table"
            data-locale="pl-PL"
            data-search="true"
            data-show-export="true">

            <thead>
              <tr>
                <th data-field="id">ID</th>
                <th data-field="sesa_no">SESA</th>
                <th data-field="first_name">Imię</th>
                <th data-field="last_name">Nazwisk</th>
                <th data-field="mob_no">Telefon</th>
                <th data-field="user_name">Nick</th>
                <th data-field="password">Hasło</th>
                <th data-field="delete">Usuń</th>
                <th data-field="update">Uaktualnij</th>
              </tr>
            </thead>
          </table>

          </div>

        </div>
        <div class="level-2">
          <p>zarządzanie menu</p>


        </div>
    </div>
    <script src="/public/admin.js"></script>
</body>
</html>